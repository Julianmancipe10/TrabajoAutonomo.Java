
public class EjemploTres {
	public static void main (String[]args) {
		//caracteres
		char letraA = 'a';          // Asigna el carácter 'a' a la variable letraA
		char letraANumerico = 61;   // Asigna el valor numérico 61 a la variable letraANumerico, que corresponde al carácter '='

		System.out.println(letraA); // Imprime 'a' en la consola
		System.out.println(letraANumerico); // Imprime '=' en la consola, porque 61 es el valor numérico del carácter '='
		// boleanos 
		boolean verdadero = true;
		boolean falso = false;
		
		System.out.println( "la letra a es = a  " + verdadero);
		System.out.println( "Messi es mejor que cristiano? " +falso);		
	}
}
